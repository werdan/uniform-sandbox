{
  "name": "component-starter-kit",
  "version": "5.0.10",
  "private": true,
  "engines": {
    "npm": ">=8.19.0",
    "node": ">=18.12.0",
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm"
  },
  "scripts": {
    "dev": "run-s pull:manifest dev:next",
    "dev:next": "next dev",
    "build:next": "next build",
    "build": "run-s pull:manifest build:next",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,scss}\"",
    "pull:manifest": "uniform context manifest download --output ./src/context/manifest.json",
    "uniform:pull:categories": "uniform canvas category pull ./content/categories",
    "uniform:pull:components": "uniform canvas component pull ./content/components",
    "uniform:pull:compositions": "uniform canvas composition pull ./content/compositions",
    "uniform:pull:project-map-definitions": "uniform project-map definition pull ./content/project-map-definitions",
    "uniform:pull:project-map-nodes": "uniform project-map node pull ./content/project-map-nodes",
    "uniform:pull": "run-s uniform:pull:components uniform:pull:categories uniform:pull:compositions uniform:pull:project-map-definitions uniform:pull:project-map-nodes",
    "uniform:push:categories": "uniform canvas category push ./content/categories",
    "uniform:push:components": "uniform canvas component push ./content/components",
    "uniform:push:compositions": "uniform canvas composition push ./content/compositions",
    "uniform:push:project-map-definitions": "uniform project-map definition push ./content/project-map-definitions",
    "uniform:push:project-map-nodes": "uniform project-map node push ./content/project-map-nodes",
    "uniform:push": "run-s uniform:push:categories uniform:push:components uniform:push:compositions uniform:push:project-map-definitions uniform:push:project-map-nodes",
    "uniform:compositions:publish": "uniform canvas composition push ./content/compositions --state published",
    "uniform:manifest:publish": "uniform context manifest publish",
    "uniform:publish": "run-s uniform:compositions:publish uniform:manifest:publish",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "postinstall": "cd cli-run && npm install && npm run build",
    "cli": "npm i && cd cli-run && node dist/index.mjs"
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^16.1.0",
    "@contentful/rich-text-types": "^16.2.0",
    "@uniformdev/canvas": "19.69.0",
    "@uniformdev/canvas-next": "19.69.0",
    "@uniformdev/canvas-react": "19.69.0",
    "@uniformdev/context": "19.69.0",
    "@uniformdev/context-next": "19.69.0",
    "@uniformdev/context-react": "19.69.0",
    "@uniformdev/project-map": "19.69.0",
    "@uniformdev/richtext": "19.69.0",
    "@vercel/og": "^0.5.7",
    "classnames": "^2.3.2",
    "daisyui": "^2.51.5",
    "framer-motion": "10.16.2",
    "next": "^13.5.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-multi-carousel": "^2.8.3",
    "react-player": "^2.12.0",
    "react-responsive-masonry": "2.1.7",
    "sass": "1.64.1",
    "sharp": "^0.32.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/addon-styling": "^1.0.3",
    "@storybook/blocks": "^7.0.7",
    "@storybook/nextjs": "^7.0.7",
    "@storybook/preview-api": "7.0.7",
    "@storybook/react": "^7.0.7",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/color": "^3.0.3",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.4",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-responsive-masonry": "2.1.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@typescript-eslint/types": "^6.4.0",
    "@uniformdev/cli": "19.69.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.47.0",
    "eslint-config-next": "13.5.6",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "prettier": "^3.0.2",
    "storybook": "^7.0.7",
    "storybook-addon-themes": "^6.1.0",
    "tailwindcss": "^3.3.1",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2"
  }
}
